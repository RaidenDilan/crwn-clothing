{
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
    // "import/resolver": {
    //   "webpack": {
    //     "config": {
    //       "resolve": {
    //         "modules": [
    //           "node_modules"
    //         ],
    //         "extensions": [
    //           ".js",
    //           ".jsx"
    //         ]
    //       }
    //     }
    //   }
    // }
  },
  "extends": [
    "react-app",
    // "plugin:import/recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    // "import",
    "react"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "ignorePatterns": [
    "/node_modules/**",
    "/build/**"
  ],
  "rules": {
    // "import/no-unresolved": [
    //   2,
    //   {
    //     "caseSensitive": true
    //   }
    // ],
    // "import/named": 2,
    // "import/namespace": 2,
    // "import/default": 2,
    // "import/export": 2,
    "indent": [
      1,
      2,
      {
        "SwitchCase": 1,
        "VariableDeclarator": 1,
        "MemberExpression": 1
      }
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "getter-return": 0,
    "default-case": [
      1,
      {
        "commentPattern": "^skip\\sdefault"
      }
    ],
    "no-unused-vars": [
      1,
      {
        "args": "none",
        "argsIgnorePattern": "req|res|next|val"
      }
      // 1,
      // {
      //   "vars": "all",
      //   "args": "after-used",
      //   "ignoreRestSiblings": false
      // }
    ],
    "jsx-quotes": [
      2,
      "prefer-single"
    ],
    "space-in-parens": [
      1,
      "never"
    ],
    "template-curly-spacing": [
      1,
      "always"
    ],
    "array-bracket-spacing": [
      1,
      "never"
    ],
    "object-curly-spacing": [
      1,
      "always"
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/prop-types": 0,
    "react/no-unescaped-entities": 0,
    "react/jsx-closing-bracket-location": [
      1,
      {
        "selfClosing": "after-props",
        // "selfClosing": 'props-aligned',
        "nonEmpty": "after-props"
      }
    ],
    "react/display-name": 0,
    "react/jsx-indent": [
      1,
      2
    ],
    "react/jsx-indent-props": [
      2,
      2
    ],
    "react/jsx-curly-spacing": [
      1,
      {
        "when": "always",
        "attributes": {
          "allowMultiline": true
        },
        "children": true
      }
    ],
    "react/jsx-tag-spacing": [
      1,
      {
        "beforeSelfClosing": "always"
      }
    ],
    "react/jsx-max-props-per-line": [
      1,
      {
        "maximum": 1
      }
    ],
    "react/jsx-first-prop-new-line": [
      1,
      "multiline"
    ]
    // "react/no-array-index-key": [1],
    // "react/jsx-no-undef": [2, { "allowGlobals": true }],
    // "semi": [0, "never"],
    // "class-methods-use-this": [2, {
    //   exceptMethods: [
    //     "render",
    //     "getInitialState",
    //     "getDefaultProps",
    //     "getChildContext",
    //     "componentWillMount",
    //     "componentDidMount",
    //     "componentWillReceiveProps",
    //     "shouldComponentUpdate",
    //     "componentWillUpdate",
    //     "componentDidUpdate",
    //     "componentWillUnmount",
    //   ],
    // }],
    // "no-undef": [2],
    // "no-case-declarations": [
    //   1
    // ],
    // "no-return-assign": [
    //   1
    // ],
    // "no-param-reassign": [
    //   1
    // ],
    // "no-shadow": [
    //   1
    // ],
    // "no-console": "off",
    // "camelcase": [
    //   1
    // ],
    // "no-underscore-dangle": [
    //   0,
    //   "always"
    // ],
    // "computed-property-spacing": [
    //   1,
    //   "always"
    // ],
    // "no-multiple-empty-lines": [
    //   2,
    //   {
    //     "max": 1,
    //     "maxEOF": 0,
    //     "maxBOF": 0
    //   }
    // ],
    // "quotes": [
    //   1,
    //   "single",
    //   "avoid-escape"
    // ],
    // "no-use-before-define": [
    //   2,
    //   {
    //     "functions": false
    //   }
    // ],
    // "prefer-const": 1,
    // "jsx-a11y/href-no-hash": "off",
    // "jsx-a11y/no-onchange": "off",
    // "jsx-a11y/click-events-have-key-events": "off",
    // "jsx-a11y/no-noninteractive-element-interactions": "off",
    // "jsx-a11y/label-has-for": "off",
    // "jsx-a11y/anchor-is-valid": [
    //   1,
    //   {
